  <script>
    fetch("/getpastdata", {
      headers: {
        'Accept': 'application/json',
        'Content-Type': 'application/json'
      },
    })
      .then((res) => {
        return res.json()
      })
      .then((res) => {
        console.log(res)

        res.map((equipments, index) => {

          let tr = document.createElement('tr');

          tr.innerHTML = `
          <tr>
          <td>
            ${equipments.userid}

</td>
<td>
  ${equipments.equipmentId}
</td>
<td>
  ${equipments.equipmentName}
</td>
<td>
  ${equipments.date_of_return}   
</td>
<td>
  ${equipments.number_of_eq} 
</td>
</tr>
<br><br>


`
          document.querySelector(".tbody").appendChild(tr);

        })








      })




  </script>

